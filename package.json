{
  "name": "quotify",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "bump": "node scripts/bump-version.js",
    "prebuild": "echo 'Prebuild step, optional version bump'",
    "clean": "rm -rf node_modules && npm cache clean --force && npm install && watchman watch-del-all && rm -rf $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && npx expo start --clear",
    "clean-install": "rmdir /s /q node_modules && yarn install && yarn expo start --reset-cache",
    "dev": "node -r dotenv/config node_modules/.bin/expo start",
    "build:android:local": "eas build --platform android --profile development --local",
    "build:ios:local": "eas build --platform ios --profile development --local",
    "build:android:preview": "yarn prebuild && eas build --platform android --profile preview",
    "build:ios:preview": "yarn prebuild && eas build --platform ios --profile preview",
    "build:android:prod:bump": "node scripts/bump-version.js $npm_config_bump && eas build --platform android --profile production",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:prod": "node scripts/bump-version.js $npm_config_bump && eas build --platform ios --profile production"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@rms-apps/ui-footer": "^1.4.1",
    "@rms-apps/ui-modal-wrapper": "^1.4.1",
    "@rms-apps/ui-themed-button": "^1.6.1",
    "@rms-apps/ui-themed-divider": "^1.5.0",
    "@rms-apps/ui-themed-switch": "^1.5.0",
    "@rms-apps/ui-themed-text": "^1.8.0",
    "@rms-apps/ui-themed-view": "^1.5.0",
    "@rms-apps/ui-utils": "^1.7.0",
    "expo": "^54.0.0",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "^6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-circular-progress": "^1.4.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "react-native-pie-chart"
        ]
      }
    }
  }
}
